
# mkdir and cd to it
mcd() {
	[[ -z "$1" ]] && { echo "Usage: $0 DIRNAME" >&2; return 1; }
	mkdir -p "$1" && cd "$1"
}

d() {
	local ds="$(dirs -v)"
	if [[ $# -eq 0 ]]; then
		echo $ds
	else
		# Get directory with the given number in the stack
		echo $ds | grep "^${1}[[:space:]]" | awk '{print $2}'
	fi
}
